<div class="container col-md-6 offset-md-3">
<form action="/createpost" method="POST" enctype="multipart/form-data"> 
    <div class="form-group">
        <label for="catname">Category Name</label>
        <input type="text" class="form-control" id="catname" name="catname" placeholder="Category Name" >
    </div>
    <div class="form-group">
        <label for="title">Title</label>
        <input type="text" class="form-control" id="title" name="title" placeholder="Title" >
    </div>
    <div class="form-group">
            <label for="image">Image</label>
            <input type="file" class="form-control" id="image" name="image" multiple="multiple" >
    </div>
    <div class="form-group">
        <label for="content">Content</label>
        <textarea class="form-control" id="content" name="content" rows="3" placeholder="content"></textarea>
    </div> 
    <div class="form-group">
        <button type="submit" class="btn btn-success"><i class="fa fa-database"></i> Submit</button>
        <a class="btn btn-info" href="/createget"><i class="fa fa-list-alt"></i> View All</a>
    </div>     
</form>
</div>


